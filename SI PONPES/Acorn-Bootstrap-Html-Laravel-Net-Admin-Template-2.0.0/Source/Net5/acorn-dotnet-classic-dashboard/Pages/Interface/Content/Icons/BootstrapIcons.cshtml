@page
@{
    Dictionary<string, string> HtmlTagData = new Dictionary<string, string>(){};
    ViewBag.HtmlTagData = HtmlTagData;

    List<string> PageStyles = new List<string>(){
      "https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.0/font/bootstrap-icons.css"
    };
    ViewBag.PageStyles = PageStyles;

    List<string> VendorScripts = new List<string>(){
      "/js/vendor/fuse.js"
    };
    ViewBag.VendorScripts = VendorScripts;

    List<string> PageScripts = new List<string>(){
      "/js/pages/interface.icons.js"
    };
    ViewBag.PageScripts = PageScripts;

    Dictionary<string, string> Breadcrumbs = new Dictionary<string, string>(){
        {"/Index","Home"},
        {"/Interface/Index","Interface"},
        {"/Interface/Content/Index","Content"},
        {"/Interface/Content/Icons/Index","Icons"}
    };
    ViewBag.Breadcrumbs = Breadcrumbs; 

    ViewBag.Title = "Bootstrap Icons"; 
    ViewBag.Description = "Free, high quality, open source icon library with over 1,300 icons. Include them anyway you likeâ€”SVGs, SVG sprite, or web fonts. Use them with or without Bootstrap in any project.";
}
<div class="container">
  <div class="row">
    <div class="col">
      <div class="page-title-container">
        <h1 class="mb-0 pb-0 display-4" id="title">@ViewBag.Title</h1>
        <partial name="_Layout.Breadcrumb"/>
      </div>

      <div class="mb-3">
        <div class="card mb-5">
          <div class="card-body">
            <p class="mb-0">@ViewBag.Description</p>
          </div>
        </div>

        <h2 class="small-title">Icons</h2>
        <div class="card mb-2">
          <div class="card-body text-center position-relative p-0">
            <input
              class="form-control borderless m-0 py-3 px-4 rounded-lg"
              type="text"
              autocomplete="off"
              placeholder="Search for icon names & tags"
              id="bootstrapIconsSearch"
            />
            <i data-acorn-icon="search" class="search-input-icon text-muted"></i>
          </div>
        </div>

        <div class="row g-2 row-cols-3 row-cols-md-4 row-cols-lg-6" id="bootstrapIconsContainer">
          <!-- Icons will be appended here -->
        </div>
      </div>
    </div>
  </div>
</div>